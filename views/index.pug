extends layout

block stylesheets
	link(rel="stylesheet", href="/stylesheets/index.css")

block content
	.container
		.row
			.card.col.s12.m10.offset-m1.l8.offset-l2.xl6.offset-xl3
				table
					thead
						tr
							th Name
					tbody
						each user in userArray
							tr.teacher(onClick="document.location.href='/" + user.initials+ "'")
								td= user.name
