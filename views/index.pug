extends layout

block stylesheets
	link(rel="stylesheet", href="/stylesheets/index.css")

block content
	.container
		.row
			.card.col.s12.m8.offset-m2.l6.offset-l3.xl4.offset-xl4
				table
					thead
						tr
							th Name
					tbody
						each user in userArray
							tr.teacher(onClick="document.location.href='/" + user.initials+ "'")
								td= user.name
